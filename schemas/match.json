{
  "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "additionalProperties": false,
      "properties": {

        "players" : {
          "type": "array",
          "items": {
              "type": "object",
              "tag" : { "type" : "string"},
              "team" : { "type" : ["string", "null"]},
              "score" : { "type" : "number"},
              "character": { "type" : ["string", "null"]},
              "grandsIndicator" : { "type" : ["string", "null"]}
          }
        },
        "bracketLoc": { 
          "type": "string",
          "default": "Winners Bracket"
        },
        "bracketLen" : { 
          "type": "string",
          "default": "Bo3"
        },
        "isVisible": {
          "type": "boolean",
          "default": false
        }
      },
    
      "required": ["players", "bracketLoc", "bracketLen", "isVisible"]
    }
