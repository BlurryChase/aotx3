{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "event" : {
      "type": "string",
      "description": "event name",
      "default" : "USW"
    },
    "game" : {
      "type": "string",
      "description": "game name",
      "default" : "SSBU"
    }
  },

  "required": ["event", "game"]
}